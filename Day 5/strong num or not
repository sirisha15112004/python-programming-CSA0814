sum1=0
num=145
temp=num
while(num):
    i=1
    f=1
    r=num%10
    while(i<=r):
        f=f*i
        i=i+1
    sum1=sum1+f
    num=num//10
if(sum1==temp):
    print(temp,"The number is a strong number")
else:
    print(temp,"The number is not a strong number")
output=145 The number is not a strong number
